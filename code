class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for pos,num in enumerate(nums):
            for posi,numi in enumerate(nums):
                if pos!=posi:
                    if num+numi == target:
                        return [pos,posi]
        
